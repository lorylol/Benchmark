        "<!-- 71161633/PMS_cpub/chart_incontent_1 -->" +
        "<div data-fuse='22384250546'></div>" +
        "<!-- 71161633/PMS_cpub/mob_incontent_1 -->" +
        "<div data-fuse='22935727507'></div></div>" +
        "</li>";

    var InsertAdHTML2 = "<li style='background-color:transparent;'>" +
